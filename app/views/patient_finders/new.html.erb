<div id='patient-finder-wrapper'>
  <div id='patient-finder'>
    <p> FIND PATIENT: </p>
    <%= form_for(:search, url: search_path, remote: true) do |form| %>
      Patient Name or ID:
      <%= form.text_field :query, placeholder: 'Patient Name or ID' %>
      <%= form.submit "Find" %>
    <% end %>
  </div>
  <div id ='patient-results'></div>
  <div id='patient-separator'></div>
</div>
